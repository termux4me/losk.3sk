termux-open-url http://localhost:1234 &
php -S localhost:1234 -t /data/data/com.termux/files/home/losk.3sk
ips=$(ifconfig|grep inet|grep -v 127|grep netmask|cut -d t -f 2|cut -d\  -f2)
php -S $ips:1234 -t /data/data/com.termux/files/home/losk.3sk
